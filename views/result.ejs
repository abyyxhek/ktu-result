<!DOCTYPE html>
<html>
<head>
  <title>Result - <%= student.name %></title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <div class="container">
    <h1>Result for <%= student.name %> (<%= student.regno %>)</h1>
    <table>
      <tr><th>Subject</th><th>Grade</th></tr>
      <% student.results.forEach(subject => { %>
        <tr>
          <td><%= subject.name %></td>
          <td><%= subject.grade %></td>
        </tr>
      <% }) %>
    </table>
    <p><strong>SGPA:</strong> <%= student.sgpa %></p>
    <a href="/">Check Another</a>
  </div>
</body>
</html>
